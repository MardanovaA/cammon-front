 <b-button type=\"submit\" variant=\"primary\" class=\"m-2\">Создать</b-button>\n        <b-button type=\"reset\" variant=\"danger\" class=\"m-2\">Очистить</b-button>\n        </div>\n      </b-form>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\nexport default {\n\nname: \"addCamera\",\n  data() {\n    return {\n      form: {\n        username: null,\n        password: null,\n        address: null,\n      },\n      show: true,\n      status: null\n    }\n  },\n  methods: {\n    onSubmit(event) {\n      event.preventDefault()\n      const vm = this;\n      axios.post('/api/camera', {\n        username: this.form.username,\n        password: this.form.password,\n        address: this.form.address\n      },{\n        headers: {\"Access-Control-Allow-Origin\": \"*\"}\n      })\n          .then(function (response) {\n            // console.log(response.data.id)\n            // this.status = response.data.id\n            vm.$router.push({name: 'synchronize', params: {id: response.data.id}})\n          })\n          .catch(function (error) {\n            console.log(error);\n          })\n    },\n    onReset(event) {\n      event.preventDefault()\n      this.form.username = null\n      this.form.password = null\n      this.form.address = null\n      this.show = false\n      this.$nextTick(() => {\n        this.show = true\n      })\n    }\n  }\n}\n</script>\n\n<style scoped>\n\n</style>"]}]}